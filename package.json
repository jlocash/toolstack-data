  },
  "scripts": {
    "build": "rm -rf ./dist/* && npx webpack --mode production",
    "start": "npx webpack-dev-server --mode development --devtool inline-source-map --host 0.0.0.0 --open --hot",
    "deploy": "npm i && npm run build && rsync -r ./dist/ root@${DEST}:/usr/lib/xui/",
    "lint": "npx eslint src --ext .js --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
